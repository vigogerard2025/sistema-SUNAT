generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Invoice {
  id              Int      @id @default(autoincrement())
  ruc             String
  tipoDoc         String
  serie           String
  numero          String
  fechaGeneracion DateTime @default(now())
  estado          String
  observaciones   String?
  createdAt       DateTime @default(now())
}